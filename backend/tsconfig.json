{
  "extends": "@strapi/typescript-utils/tsconfigs/server",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": ["./src/**/*.ts", "./src/**/*.js", "./config/**/*.ts", "./config/**/*.js"],
  "exclude": ["node_modules", "dist", ".cache", "build", ".tmp"]
}
